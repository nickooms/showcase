{
  "name": "showcase",
  "version": "1.0.0",
  "description": "Nick Ooms Showcase",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.dev.js --open",
    "build": "webpack --config webpack.config.prod.js",
    "test": "jest --watch",
    "test:coverage": "jest --coverage --colors",
    "lint": "eslint --cache --format codeframe --ext js,jsx src",
    "jest:visual": "jest -c jest-puppeteer.config.js",
    "test:visual": "start-server-and-test storybook http-get://localhost:9009 jest:visual",
    "storybook": "start-storybook -p 9009 -s public --ci"
  },
  "keywords": [
    "nick",
    "ooms",
    "showcase",
    "react",
    "material-design"
  ],
  "author": "Nick Ooms <oomsni@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.40",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0-beta.0",
    "@storybook/addon-actions": "^5.1.10",
    "@storybook/addon-knobs": "^5.1.10",
    "@storybook/addon-storysource": "^5.1.10",
    "@storybook/react": "^5.1.10",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.0-beta.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^2.1.1",
    "dotenv-webpack": "^1.7.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.14.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^4.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^29.5.0",
